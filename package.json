  "scripts": {
    "build:production": "ng run web-frame:build:production --progress=false",
    "e2e": "ng run web-frame:e2e",
    "e2e:ci": "ng run web-frame:e2e:ci",
    "lint": "ng lint && npm run lint:style",
    "lint:ci": "ng lint --configuration=ci && npm run lint:style",
    "lint:style": "stylelint './web-frame/src/**/*.*css'",
