  "scripts": {
    "build:production": "ng run web-frame:build:production --progress=false",
    "e2e": "ng run web-frame:e2e",
    "e2e:ci": "ng run web-frame:e2e:ci",
    "lint": "npm run lint:code; npm run lint:style",
    "lint:ci": "npm run lint:code:ci; npm run lint:style",
    "lint:code": "ng lint",
    "lint:code:ci": "ng lint --configuration=ci",
    "lint:code:fix": "ng lint --configuration=fix",
    "lint:style": "stylelint 'web-frame/**/*.*css'",
    "lint:style:fix": "stylelint --fix 'web-frame/**/*.*css'",
    "ngcc": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points --no-async",
    "postinstall": "npm run ngcc",
    "start": "ng serve",
    "start:proxy": "ng serve --configuration=proxy",
    "test": "ng test",
    "test:ci": "ng test --configuration=ci",
    "webdriver-update": "webdriver-manager update --gecko=false --chrome=false"
