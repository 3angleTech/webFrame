{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "defaultProject": "web-frame",
  "newProjectRoot": "projects",
  "projects": {
    "web-frame": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "export": true,
          "changeDetection": "OnPush",
          "flat": false,
          "style": "scss"
        },
        "@schematics/angular:directive": {
          "export": true,
          "flat": false
        },
        "@schematics/angular:pipe": {
          "export": true,
          "flat": true
        },
        "@schematics/angular:service": {
          "export": true,
          "flat": true
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "index": "src/index.html",
            "main": "src/main.ts",
            "outputPath": "dist",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "scripts": [],
            "styles": [
              {
                "bundleName": "vendor-styles",
                "input": "node_modules/normalize.css/normalize.css"
              },
              {
                "bundleName": "vendor-styles",
                "input": "node_modules/material-design-icons/iconfont/material-icons.css"
              },
              {
                "bundleName": "main-styles",
                "input": "src/styles/main.scss"
              },
              {
                "bundleName": "print-styles",
                "input": "src/styles/print.scss"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles/includes"
              ]
            }
          },
          "configurations": {
            "production": {
              "buildOptimizer": true,
              "extractCss": true,
              "extractLicenses": true,
              "namedChunks": false,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/app/app-routes.ts",
                  "with": "src/app/app-routes.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "web-frame:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "web-frame:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web-frame:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "scripts": [],
            "styles": [
              {
                "bundleName": "vendor-styles",
                "input": "node_modules/normalize.css/normalize.css"
              },
              {
                "bundleName": "vendor-styles",
                "input": "node_modules/material-design-icons/iconfont/material-icons.css"
              },
              {
                "bundleName": "main-styles",
                "input": "src/styles/main.scss"
              },
              {
                "bundleName": "print-styles",
                "input": "src/styles/print.scss"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles/includes"
              ]
            }
          },
          "configurations": {
            "ci": {
              "browsers": "ChromeHeadlessCI",
              "progress": false,
              "watch": false
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "format": "stylish",
            "tslintConfig": "tslint.json",
            "tsConfig": [
              "e2e/tsconfig.json",
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          },
          "configurations": {
            "ci": {
              "tslintConfig": "tslint.ci.json"
            }
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "devServerTarget": "web-frame:serve",
            "protractorConfig": "e2e/protractor.conf.js"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "web-frame:serve",
              "protractorConfig": "e2e/protractor-ci.conf.js"
            }
          }
        }
      }
    }
  }
}
