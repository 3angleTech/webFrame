{
  "compileOnSave": false,
  "angularCompilerOptions": {
    "enableIvy": true,
    "fullTemplateTypeCheck": false,
    "strictInjectionParameters": false,
    "strictTemplates": false
  },
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "strictNullChecks": false,
    "target": "ES2015",
    "lib": [
      "DOM",
      "ES2018"
    ],
    "paths": {
      "~app-feature/account-feature": [
        "./web-frame/src/app-feature/account-feature/index.ts"
      ],
      "~app-feature/profile-feature": [
        "./web-frame/src/app-feature/profile-feature/index.ts"
      ],
      "~app-feature/sandbox-feature": [
        "./web-frame/src/app-feature/sandbox-feature/index.ts"
      ],
      "~app-shared/config": [
        "./web-frame/src/app-shared/config/index.ts"
      ],
      "~app-shared/core": [
        "./web-frame/src/app-shared/core/index.ts"
      ],
      "~app-shared/data-table": [
        "./web-frame/src/app-shared/data-table/index.ts"
      ],
      "~app-shared/security": [
        "./web-frame/src/app-shared/security/index.ts"
      ],
      "~app-shared/translate": [
        "./web-frame/src/app-shared/translate/index.ts"
      ],
      "~app-shared/test-utils": [
        "./web-frame/src/app-shared/test-utils/index.ts"
      ],
      "~app-shared/utils": [
        "./web-frame/src/app-shared/utils/index.ts"
      ],
      "~app-shell/default-shell": [
        "./web-frame/src/app-shell/default-shell/index.ts"
      ],
      "~app-shell/minimal-shell": [
        "./web-frame/src/app-shell/minimal-shell/index.ts"
      ]
    }
  }
}
