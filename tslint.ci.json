{
  "extends": [
    "./tslint.json"
  ],
  "rules": {
    /* NOTE: These rules are disabled in order for the CI pipelines to pass. IDEs use stricter rules. In time, these overrides need to be removed. */
    "comment-format": false,
    "interface-name": false,
    "jsdoc-format": false,
    "newline-before-return": false,
    "no-magic-numbers": false,
    "no-null-keyword": false,
    "no-suspicious-comment": false,
    "no-unbound-method": false,
    "no-unsafe-any": false,
    "template-cyclomatic-complexity": false,
    "pipe-prefix": false,
    "prefer-readonly": false,
    "template-no-call-expression": false,
    "template-use-track-by-function": false,
    "variable-name": false
  }
}
